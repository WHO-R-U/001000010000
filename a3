#include <stdio.h>
int main()
{
    //float f,c;
    //f = 64.0;
    //c = (5.0/9)*(f - 32);
    //printf("f = %f\nc = %f\n",f,c);
    
    float po = 1000,r1 = 0.0036,r2 = 0.0225,r3 = 0.0198,p1,p2,p3;
    p1 = po*(1+r1);
    p2 = po*(1+r2);
    p3 = po*(1+r3/2)*(1+r3/2);
    printf("p1 = %f\np2 = %f\np3 = %f\n",p1,p2,p3);
    return 0;
}
